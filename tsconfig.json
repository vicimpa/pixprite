{
  "compilerOptions": {
    "tsBuildInfoFile": null,
    "target": "esnext",
    "useDefineForClassFields": true,
    "lib": [
      "esnext",
      "DOM",
      "DOM.Iterable"
    ],
    "module": "ESNext",
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "verbatimModuleSyntax": true,
    "moduleDetection": "force",
    "noEmit": true,
    "jsx": "react-jsx",
    "strict": true,
    "experimentalDecorators": true,
    "types": [
      "vite/client",
      "vite-plugin-glsl/ext"
    ],
    "paths": {
      "$*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "src"
  ]
}